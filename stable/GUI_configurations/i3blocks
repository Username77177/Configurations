separator_block_width=15
markup=pango
# Переменной $BLOCK_NAME присваивается имя блока, в котором она выполняется
command=~/Configurations/i3/i3blocks-scripts/$BLOCK_NAME
# Для вставки в span'ы


[time]
# Тут просто будет исполняться файл time в ~/Configurations/i3/i3blocks-scripts/

full_text=<span background='fefefe'>$command</span>

interval=5
